{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "cdn",
	"main": "src/index.ts",
	"compatibility_date": "2025-09-02",
	"vars": {
		"BASE_URL": "https://cdn.artlu.xyz"
	},
	"assets": {
		"binding": "ASSETS",
		"directory": "./public"
	},
	"kv_namespaces": [
		{
			"binding": "image_metadata",
			"id": "ee46dceb3e8b4d919e2fc4c91081209b"
		}
	],
	"r2_buckets": [
		{
			"bucket_name": "cdn-images",
			"binding": "cdn_images"
		}
	],
	"images": {
		"binding": "IMAGES"
	}
}
